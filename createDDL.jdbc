CREATE TABLE ABSTRACTMODEL (ID BIGINT NOT NULL, DTYPE VARCHAR(31), CREATEDAT TIMESTAMP, UPDATEDAT TIMESTAMP, VERSION BIGINT, CITY VARCHAR, COUNTRY VARCHAR, STREET VARCHAR, ZIP VARCHAR, CART LONGVARBINARY, EXPENDITURE DOUBLE, FIRSTNAME VARCHAR, INVENTORY LONGVARBINARY, LASTNAME VARCHAR, MAIL VARCHAR, ADDRESS_ID BIGINT, ACTUALCOSTS DOUBLE, BUDGET DOUBLE, EVENTNAME VARCHAR, MAXCAPACITY INTEGER, PRIMARY KEY (ID))
CREATE TABLE ABSTRACTMODEL_ABSTRACTMODEL (Address_ID BIGINT NOT NULL, Clients_ID BIGINT NOT NULL, PRIMARY KEY (Address_ID, Clients_ID))
ALTER TABLE ABSTRACTMODEL ADD CONSTRAINT FK_ABSTRACTMODEL_ADDRESS_ID FOREIGN KEY (ADDRESS_ID) REFERENCES ABSTRACTMODEL (ID)
ALTER TABLE ABSTRACTMODEL_ABSTRACTMODEL ADD CONSTRAINT FK_ABSTRACTMODEL_ABSTRACTMODEL_Address_ID FOREIGN KEY (Address_ID) REFERENCES ABSTRACTMODEL (ID)
ALTER TABLE ABSTRACTMODEL_ABSTRACTMODEL ADD CONSTRAINT FK_ABSTRACTMODEL_ABSTRACTMODEL_Clients_ID FOREIGN KEY (Clients_ID) REFERENCES ABSTRACTMODEL (ID)
